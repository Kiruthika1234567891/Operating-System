#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>

int main() {
    pid_t pid = fork();
    if (pid == 0) 
    {
        
        printf("Child PID: %d\n", getpid());
        printf("Parent PID: %d\n", getppid());
    } else if (pid > 0) {
       
        printf("Parent PID: %d\n", getpid());
    } else {
        
        perror("Fork failed");
    }
    return 0;
}
